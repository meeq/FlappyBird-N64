{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB Remote Debug (N64)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/FlappyBird.elf",
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "targetArchitecture": "mips",
            "miDebuggerPath": "${workspaceFolder}/toolchain/bin/mips64-elf-gdb",
            "miDebuggerServerAddress": "localhost:9123",
            "setupCommands": [
                {
                    "description": "Set architecture to MIPS R4300",
                    "text": "set architecture mips:4300",
                    "ignoreFailures": false
                },
                {
                    "description": "Enable pretty-printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "stopAtEntry": false
        }
    ]
}
